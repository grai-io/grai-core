{
  "name": "grai-frontend",
  "version": "0.1.2",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.7.2",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@graphql-tools/mock": "^8.7.14",
    "@graphql-tools/schema": "^9.0.12",
    "@mui/icons-material": "^5.10.16",
    "@mui/lab": "^5.0.0-alpha.105",
    "@mui/material": "^5.10.11",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.0",
    "@types/apollo-upload-client": "^17.0.2",
    "@types/jest": "^27.5.2",
    "@types/luxon": "^3.2.0",
    "@types/node": "^16.18.2",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "@types/react-router-dom": "^5.3.3",
    "apollo-upload-client": "^17.0.0",
    "bundlewatch": "^0.3.3",
    "casual": "^1.6.2",
    "dayjs": "^1.11.6",
    "elkjs": "^0.8.2",
    "eslint": "^8.35.0",
    "eslint-plugin-import": "^2.27.5",
    "graphql": "^16.6.0",
    "luxon": "^3.2.1",
    "material-ui-popup-state": "^5.0.4",
    "notistack": "^2.0.8",
    "posthog-js": "^1.45.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.0.0",
    "react-scripts": "5.0.1",
    "reactflow": "^11.0.0",
    "source-map-explorer": "^2.5.3",
    "typescript": "^4.8.4",
    "web-vitals": "^2.1.4",
    "web-worker": "^1.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{ts,tsx,test.tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx,test.tsx} --fix",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "bundlewatch": "bundlewatch",
    "download:schema": "npx apollo client:download-schema --endpoint=http://127.0.0.1:8000/graphql/ src/testing/schema.json",
    "generate:types": "npx apollo client:codegen --target typescript --localSchemaFile src/testing/schema.json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bundlewatch": {
    "files": [
      {
        "path": "build/**/*.js",
        "maxSize": "500kB"
      }
    ]
  }
}
